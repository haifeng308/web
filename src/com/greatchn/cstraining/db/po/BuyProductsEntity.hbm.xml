<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.greatchn.cstraining.db.po.BuyProductsEntity" table="buy_products" schema="cstraining">
        <id name="id" column="ID"/>
        <property name="ordersId" column="ordersId"/>
        <property name="productstype" column="productstype"/>
        <property name="productPrice" column="product_price"/>
        <property name="subjectId" column="SUBJECT_id"/>
        <property name="subjectClassId" column="subject_class_id"/>
        <property name="outlineId" column="outline_id"/>
        <property name="bz" column="bz"/>
    </class>
</hibernate-mapping>